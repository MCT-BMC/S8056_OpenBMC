From 23694d52aa03d44d7291a66ca4a7af54ef33ca9f Mon Sep 17 00:00:00 2001
From: John Chung <john.chung@mic.com.tw>
Date: Mon, 19 Dec 2022 11:28:03 +0800
Subject: [PATCH] Add bmc status interface supported

---
 gen/xyz/openbmc_project/OEM/BmcStatus/meson.build | 13 +++++++++++++
 gen/xyz/openbmc_project/OEM/meson.build           | 15 +++++++++++++++
 xyz/openbmc_project/OEM/BmcStatus.interface.yaml  |  9 +++++++++
 3 files changed, 37 insertions(+)
 create mode 100644 gen/xyz/openbmc_project/OEM/BmcStatus/meson.build
 create mode 100644 xyz/openbmc_project/OEM/BmcStatus.interface.yaml

diff --git a/gen/xyz/openbmc_project/OEM/BmcStatus/meson.build b/gen/xyz/openbmc_project/OEM/BmcStatus/meson.build
new file mode 100644
index 0000000..048516b
--- /dev/null
+++ b/gen/xyz/openbmc_project/OEM/BmcStatus/meson.build
@@ -0,0 +1,13 @@
+# Generated file; do not modify.
+generated_sources += custom_target(
+    'xyz/openbmc_project/OEM/BmcStatus__cpp'.underscorify(),
+    input: [ meson.source_root() / 'xyz/openbmc_project/OEM/BmcStatus.interface.yaml',  ],
+    output: [ 'server.cpp', 'server.hpp', 'client.hpp',  ],
+    command: [
+        sdbuspp_gen_meson_prog, '--command', 'cpp',
+        '--output', meson.current_build_dir(),
+        '--tool', sdbusplusplus_prog,
+        '--directory', meson.source_root(),
+        'xyz/openbmc_project/OEM/BmcStatus',
+    ],
+)
diff --git a/gen/xyz/openbmc_project/OEM/meson.build b/gen/xyz/openbmc_project/OEM/meson.build
index c94cba7..998bd48 100755
--- a/gen/xyz/openbmc_project/OEM/meson.build
+++ b/gen/xyz/openbmc_project/OEM/meson.build
@@ -57,4 +57,19 @@ generated_others += custom_target(
         'xyz/openbmc_project/OEM/HostStatus',
     ],
     build_by_default: true,
+)
+
+subdir('BmcStatus')
+generated_others += custom_target(
+    'xyz/openbmc_project/OEM/BmcStatus__markdown'.underscorify(),
+    input: [ meson.source_root() / 'xyz/openbmc_project/OEM/BmcStatus.interface.yaml',  ],
+    output: [ 'BmcStatus.md' ],
+    command: [
+        sdbuspp_gen_meson_prog, '--command', 'markdown',
+        '--output', meson.current_build_dir(),
+        '--tool', sdbusplusplus_prog,
+        '--directory', meson.source_root(),
+        'xyz/openbmc_project/OEM/BmcStatus',
+    ],
+    build_by_default: true,
 )
\ No newline at end of file
diff --git a/xyz/openbmc_project/OEM/BmcStatus.interface.yaml b/xyz/openbmc_project/OEM/BmcStatus.interface.yaml
new file mode 100644
index 0000000..d267587
--- /dev/null
+++ b/xyz/openbmc_project/OEM/BmcStatus.interface.yaml
@@ -0,0 +1,9 @@
+description: >
+    Implement to provide status for bmc attributes.
+
+properties:
+    - name: bootSourceStatus
+      type: int32
+      description: >
+         Setting of BMC booting soruce status
+# vim: tabstop=8 expandtab shiftwidth=4 softtabstop=4
\ No newline at end of file
-- 
