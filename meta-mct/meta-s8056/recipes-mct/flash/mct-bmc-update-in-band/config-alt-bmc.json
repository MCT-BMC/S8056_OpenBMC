[{
        "blob": "/flash/backup",
        "handler": {
                "type": "file",
                "path": "/run/initramfs/bmc-backup"
        },
        "actions": {
                "preparation": {
                        "type": "systemd",
                        "unit": "phosphor-ipmi-flash-bmc-prepare.target"
                },
                "verification": {
                        "type": "systemd",
                        "unit": "phosphor-ipmi-flash-alt-bmc-verify.service"
                },
                "update": {
                        "type": "systemd",
                        "unit": "phosphor-ipmi-flash-alt-bmc-update.service"
                }
        }
}]

